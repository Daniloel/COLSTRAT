<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="COLSTRAT.App"
             xmlns:infra="clr-namespace:COLSTRAT.Infraestructure">
    <Application.Resources>
        <ResourceDictionary>
            <!--Locator-->
            <infra:InstanceLocator x:Key="Locator"></infra:InstanceLocator>
            <!--Colours-->
            <Color x:Key="MainColor">#F1CD81</Color>
            <Color x:Key="FontColor">#100D0D</Color>
            <Color x:Key="MenuColor">#D9855A</Color>
            <Color x:Key="MenuFontColor">#F5D5B2</Color>
            <Color x:Key="BackgroundColor">#F5D5B2</Color>
            <Color x:Key="AccentColor">#BF533B</Color>
        </ResourceDictionary> 
    </Application.Resources>
</Application>