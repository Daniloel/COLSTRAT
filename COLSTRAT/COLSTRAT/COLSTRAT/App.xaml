<?xml version="1.0" encoding="utf-8"?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="COLSTRAT.App"
             xmlns:infra="clr-namespace:COLSTRAT.Infraestructure">
    <Application.Resources>
        <ResourceDictionary>
            <!--Locator-->
            <infra:InstanceLocator x:Key="Locator"></infra:InstanceLocator>
            <!--Colours-->
            <Color x:Key="MainColor">#03263E</Color><!--nav bar-->
            <Color x:Key="FontColor">#100D0D</Color><!--negro-->
            <Color x:Key="MenuColor">#08375A</Color><!--fondo del menu-->
            <Color x:Key="MenuFontColor">#FFFFFF</Color><!--letras del menu-->
            <Color x:Key="BackgroundColor">#F3F4EB</Color><!--background-->
            <Color x:Key="AccentColor">#C21120</Color>
            <!--rojo-->
            <Color x:Key="AccentColor2">#741424</Color>
            <!--rojo-->

            <!--Parameters  -->
            <x:String x:Key="URL_API">http://colstrat-api.somee.com</x:String>
        </ResourceDictionary> 
    </Application.Resources>
</Application>