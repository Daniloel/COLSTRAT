<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="COLSTRAT.App"
             xmlns:infra="clr-namespace:COLSTRAT.Infraestructure">
    <Application.Resources>
        <ResourceDictionary>
            <!--Locator-->
            <infra:InstanceLocator x:Key="Locator"></infra:InstanceLocator>
            <!--Colours-->
            <Color x:Key="MainColor">#F1CD81</Color><!--color anemico-->
            <Color x:Key="FontColor">#100D0D</Color><!--negro-->
            <Color x:Key="MenuColor">#D9855A</Color><!--fondo del menu-->
            <Color x:Key="MenuFontColor">#F5D5B2</Color><!--letras del menu-->
            <Color x:Key="BackgroundColor">#F5D5B2</Color><!--color carne-->
            <Color x:Key="AccentColor">#BF533B</Color>
            <!--rojo-->
            <Color x:Key="AccentColor2">#BF533B</Color>
            <!--rojo-->

            <!--Parameters-->
            <x:String x:Key="URL_API">http://colstrat-api.somee.com</x:String>
        </ResourceDictionary> 
    </Application.Resources>
</Application>